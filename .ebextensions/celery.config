packages:
    yum:
        gcc: []
        libcurl-devel: []
        python3-pycurl: []

commands:
    celery_start:
       command: |
           #!/usr/bin/env bash
           cd /opt/python/current/app
           . /opt/python/current/env
           source /opt/python/run/venv/bin/activate
           celery -A worker:celery worker -l INFO
